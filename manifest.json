{
    "manifest_version": 2,
    "name": "Style Stealer",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery.js", "jquery.tipsy.js", "content.js"],
            "css": ["tipsy.css"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}