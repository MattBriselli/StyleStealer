{
    "manifest_version": 2,
    "name": "Style Stealer",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery.js","jquery.tipsy.js","content.js"],
            "css": ["tipsy.css","stealer.css"]
        }
    ],
    "browser_action": {
        "default_icon": "stealer.png"
    }
}